{
    "name": "jasny/entity",
    "type": "library",
    "description": "Object representation for stateful entities",
    "keywords": [],
    "license": "MIT",
    "homepage": "http://jasny.github.com/entity",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/entity/issues",
        "source": "https://github.com/jasny/entity"
    },
    "require": {
        "php": ">=7.1.0",
        "jasny/php-functions": "^4.0"
    },
    "conflicts": {
        "jasny/meta": "< 3.0 || >= 4.0",
        "jasny/typecast": "< 2.1 || >= 4.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3.5",
        "jasny/meta": "^3.0",
        "jasny/typecast": "dev-master as 2.1.0",
        "phpstan/phpstan": "0.9.2"
    },
    "suggest": {
        "jasny/entity-collection": "Data structures for entities",
        "jasny/meta": "Use annotations",
        "jasny/typecast": "Cast values based on entity metadata"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Entity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Entity\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse src",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
