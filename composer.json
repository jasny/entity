{
    "name": "jasny/entity-collection",
    "type": "library",
    "description": "Data structures for Entities",
    "keywords": [],
    "license": "MIT",
    "homepage": "http://jasny.github.com/entity-collection",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/entity-collection/issues",
        "source": "https://github.com/jasny/entity-collection"
    },
    "require": {
        "php": ">=7.1.0",
        "jasny/php-functions": "^4.0.2",
        "jasny/entity": "dev-master"
    },
    "conflicts": {
        "jasny/meta": "< 3.0 || >= 4.0",
        "jasny/typecast": "< 2.1 || >= 4.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3",
        "phpstan/phpstan": "0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\EntityCollection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\EntityCollection\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse src --level=4 --configuration=phpstan.neon",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
