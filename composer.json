{
    "name": "jasny/entity",
    "type": "library",
    "description": "Object representation for stateful entities",
    "keywords": [],
    "license": "MIT",
    "homepage": "http://jasny.github.com/entity",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/entity/issues",
        "source": "https://github.com/jasny/entity"
    },
    "require": {
        "php": ">=7.1.0",
        "jasny/php-functions": "^4.0"
    },
    "conflicts": {
        "jasny/meta": "< 3.0 || >= 4.0",
        "jasny/typecast": "< 2.1 || >= 4.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3",
        "jasny/meta": "^3.0",
        "jasny/typecast": "dev-master as 2.1.0"
    },
    "suggest": {
        "jasny/meta": "Use annotations",
        "jasny/typecast": "Cast values based on entity metadata"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Entity\\": "src/Entity",
            "Jasny\\EntityCollection\\": "src/EntityCollection"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
